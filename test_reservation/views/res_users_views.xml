<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_users_form_reservation" model="ir.ui.view">
        <field name="name">res.users.view.form.inherit.reservation</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@class='oe_title']//field[@name='partner_id']" position="after">
                <field name="main_order_id"/>
            </xpath>
        </field>
    </record>
</odoo>
